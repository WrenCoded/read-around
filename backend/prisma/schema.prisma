generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Book{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  author String
  owner User @relation(fields: [ownerId], references: [id])
  ownerId String @db.ObjectId
  genre String?
}

model User{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  usernames String
  name String
  books Book[]
}


